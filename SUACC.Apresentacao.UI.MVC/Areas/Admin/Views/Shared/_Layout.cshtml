<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>SUACC - Sistema Único de Atendimento Call Center</title>

    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet" type="text/css">
    @Styles.Render("~/Content/globalcss")
    @Scripts.Render("~/Content/globaljs")

</head>
<body>
    <!-- Main navbar -->
    <div class="navbar navbar-inverse">
        <div class="navbar-header">
            <a class="navbar-brand" href="index.html"><img src="assets/images/logo_light.png" alt=""></a>
            <ul class="nav navbar-nav visible-xs-block">
                <li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
                <li><a class="sidebar-mobile-main-toggle"><i class="icon-paragraph-justify3"></i></a></li>
            </ul>
        </div>
        <div class="navbar-collapse collapse bg-blue-800" id="navbar-mobile">
            <ul class="nav navbar-nav">
                @*<li><a class="sidebar-control sidebar-main-toggle hidden-xs"><i class="icon-paragraph-justify3"></i></a></li>*@
                <li>
                    <a href="/Home/Index">
                        <i class="icon-home"></i>
                        <span class="visible-xs-inline-block position-right">Home</span>
                    </a>
                </li>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                @Html.Action("DadosUsuario", "Home", new { area = "" })
            </ul>
        </div>
    </div>
    <!-- /main navbar -->
    <!-- Page container -->
    <div class="page-container">
        <!-- Page content -->
        <div class="page-content">

            <!-- Main content -->
            <div class="content-wrapper">

                @RenderBody()

            </div>
            <!-- /main content -->
        </div>
        <!-- /page content -->
    </div>
    <!-- /page container -->
    <!-- Vertical form modal -->
    <div id="modal_form_vertical" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h5 class="modal-title">Vertical form</h5>
                </div>

            </div>
        </div>
    </div>
    <!-- /vertical form modal -->



    @Scripts.Render("~/Content/globalthemajs")

    <script type="text/javascript" src="~/Content/assets/js/plugins/notifications/bootbox.min.js"></script>


    <!-- Theme JS files -->

    <script type="text/javascript" src="~/Content/assets/js/pages/form_checkboxes_radios.js"></script>
    <script type="text/javascript" src="~/Content/assets/js/plugins/notifications/sweet_alert.min.js"></script>
    <script type="text/javascript" src="~/Content/assets/js/plugins/forms/selects/select2.min.js"></script>
    <script type="text/javascript" src="~/Content/assets/js/pages/components_modals.js"></script>

    <!-- /theme JS files -->

    @RenderSection("scripts", required: false)
    @{ Html.RenderPartial("_Modal"); }

</body>
</html>
